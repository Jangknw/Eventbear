@model BookingPlaceDetailView

@{
    ViewBag.Title = "Booking";
}

        <div class="container-fluid">
        <div class="row matop">
            <div class="col-md-12">
                <h2 class="search NameEvent">รายการจอง</h2>
            </div>
        </div>
    </div>


@Html.Action("_PartialIndexBooking", "Booking", new { Customer_ID = 100002 }) 
    


